  "scripts": {
    "update": "ssh ubuntu@wedding './wedding-update.sh'",
    "heroku": "heroku container:login && heroku container:push web && heroku container:release web",
    "hon": "heroku scale web=1",
    "hoff": "heroku scale web=0",
    "logh": "heroku logs -t",
    "log": "ssh-add ~/.ssh/id_rsa; ssh ubuntu@wedding docker logs -f wedding",
